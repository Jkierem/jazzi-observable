{
    "name": "jazzi-observable",
    "version": "1.0.3-alpha",
    "main": "index.js",
    "author": "Juan Gomez",
    "license": "MIT",
    "private": false,
    "files": [
        "index.js",
        "index.d.ts",
        "operators.js",
        "operators.d.ts",
        "schedulers.js",
        "schedulers.d.ts",
        "constructors.js",
        "constructors.d.ts"
    ],
    "dependencies": {
        "jazzi": "^1.5.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-polyfill": "^6.26.0",
        "gulp": "^4.0.2",
        "jest": "^26.6.3",
        "ramda": "^0.27.1",
        "webpack": "^5.30.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "build": "gulp build",
        "build:dev": "gulp dev",
        "clean": "gulp clean",
        "start": "gulp watch",
        "test": "rm -rf ./coverage && jest --coverage --verbose",
        "test:watch": "jest --verbose --watch",
        "prepublish": "yarn build"
    },
    "jest": {
        "verbose": true,
        "collectCoverageFrom": [
            "src/*.{js,jsx}",
            "src/*/*.{js,jsx}"
        ],
        "testMatch": [
            "**/__tests__/(*.)test.js"
        ],
        "testEnvironment": "node",
        "transform": {
            ".js$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "moduleDirectories": [
            "node_modules",
            "modules"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "jsx"
        ],
        "setupFilesAfterEnv": [
            "./setupTests.js"
        ]
    }
}

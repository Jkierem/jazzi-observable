{
    "name": "jazzi-observable",
    "version": "1.1.1",
    "main": "index.js",
    "author": "Juan Gomez",
    "license": "MIT",
    "private": false,
    "types": "index.d.ts",
    "files": [
        "index.js",
        "index.d.ts",
        "operators.js",
        "operators.d.ts",
        "schedulers.js",
        "schedulers.d.ts",
        "constructors.js",
        "constructors.d.ts"
    ],
    "dependencies": {
        "jazzi": "latest"
    },
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "jest": "^27.1.1",
        "ts-jest": "^27.0.5",
        "typescript": "^4.5.5"
    },
    "scripts": {
        "build": "yarn build:ts && yarn build:deno",
        "build:ts": "tsc",
        "build:deno": "deno run --unstable --allow-read --allow-write scripts/build.js",
        "clean": "rm -rf ./dist && rm -rf ./deno && rm -rf ./coverage",
        "test": "rm -rf ./coverage && jest --coverage --verbose",
        "test:silent": "rm -rf ./coverage && jest --silent",
        "test:watch": "jest --verbose --watch",
        "prepublish": "yarn clean && yarn build"
    }
}
